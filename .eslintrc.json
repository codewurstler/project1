{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
    "object-curly-spacing": "warn",
    "prefer-const": "warn",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off",
    "class-methods-use-this": "off",
    "max-classes-per-file": "off",
    "no-console": "off"
  }
}
